generator client {
  provider   = "prisma-client-js"
  output     = "../generated/client"
  engineType = "client"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator dbml {
  provider = "prisma-dbml-generator"
  output   = "../generated/dbml"
}
