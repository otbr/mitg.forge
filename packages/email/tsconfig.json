{
	"$schema": "https://json.schemastore.org/tsconfig",
	"include": ["src"],
	"exclude": ["dist", "node_modules"],
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"strict": true,
		"skipLibCheck": true,
		"verbatimModuleSyntax": true,
		"outDir": "dist"
	}
}
