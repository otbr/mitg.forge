{
	"$schema": "https://turbo.build/schema.json",
	"ui": "stream",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", "out/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"types": {
			"dependsOn": ["^types"]
		},
		"check": {
			"dependsOn": ["^check"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
